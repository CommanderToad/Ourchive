<Alloy>
	<View id="homeView">
		<View id="addView">
			<View class="buttonRow" id="row1">
				<ImageView class="leftBtn" image="images/icon-photos.png" onClick="addPhoto"></ImageView>
				<ImageView class="rightBtn" image="images/icon-video.png" onClick="addVideo"></ImageView>
			</View>
			<View class="buttonRow" id="row2">
				<ImageView class="leftBtn" image="images/icon-text.png" onClick="addText"></ImageView>
				<ImageView class="rightBtn" image="images/icon-audio.png"></ImageView>
			</View>
			<View class="buttonRow" id="row3">
				<Button id="myOurchiveBtn" onClick="myOurchive">MyStream</Button>
			</View>
		</View>
		<View id="tableHeader">
			<Label id="tableTitle">Events</Label>
			<Button id="addEvent">New +</Button>
		</View>
		<TableView id="events">
		</TableView>
	</View>
</Alloy>